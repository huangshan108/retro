<div class="title">Issue List</div>
<div class="issue-list">
  <% @issues.each do |issue| %>
    <div class="issue-box">
      <span><%= issue.detail %></span>
      <% if is_vote %>
        <span class="vote">
          <input type="number" data-issue-id="<%= issue.id %>" value="0" min="0" max="5">
        </span>
      <% end %>
    </div>
  <% end %>
</div>