<!DOCTYPE html>
<html>
<head>
  <title>Retro</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:notice] %>
 <div class="notice"><%= flash[:notice] %></div>
<% elsif flash[:error] %>
	<div class="error"><%= flash[:error] %></div>
<% end %>
<%= yield %>

<%= render 'session/app' %>
</body>
</html>
